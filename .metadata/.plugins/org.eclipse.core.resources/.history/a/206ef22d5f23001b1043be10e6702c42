package entities;

public class FuncTerceirizado extends Funcionario{
	
	private double despezaAdicional = 1.1;
	
	public FuncTerceirizado(String nome, int horas, double valorPorHora) {
		super(nome, horas, valorPorHora);
	}
	
	@Override
	public double pagamento() {
		return this.horas * this.valorPorHora;
	}
}
