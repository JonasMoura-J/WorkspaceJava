package entities;

public class Retangulo {
	public String nome;
	public double glossSalary;
	public double tax;
	
	public double area() {
		return width * height;
	}
	
	public double perimeter() {
		return width*2 + height*2;
	}
	
	public double diagonal() {
		return Math.sqrt((width*width) + (height*height));
	}
}
